<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mlabs.bbm.comewithme">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.mlabs.bbm.comewithme.SplashScreen"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
             </intent-filter>
        </activity>

        <activity
            android:name="com.mlabs.bbm.comewithme.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="com.mlabs.bbm.firstandroidapp.MainActivity"/>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.mlabs.bbm.comewithme.Activity">
            <intent-filter>
                <action android:name="com.mlabs.bbm.firstandroidapp.Activity" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.mlabs.bbm.comewithme.SignUp">
            <intent-filter>
                <action android:name="com.mlabs.bbm.firstandroidapp.Activity" />
            </intent-filter>
        </activity>

    </application>

</manifest>